<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.puzzle.appname.numeros5">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/a"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/eleventhousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/b"
                android:layout_below="@+id/a"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/fifteenthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/c"
                android:layout_below="@+id/b"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/eighteeenthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/d"
                android:layout_below="@+id/c"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/twentytwothousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/e"
                android:layout_below="@+id/d"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/twentyeightthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/f"
                android:layout_below="@+id/e"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/thirtyseventhousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/g"
                android:layout_below="@+id/f"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/eightyfivethousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/h"
                android:layout_below="@+id/g"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/hundredthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/i"
                android:layout_below="@+id/h"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/onezeroeightthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/j"
                android:layout_below="@+id/i"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/onehundredsixtythousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/k"
                android:layout_below="@+id/j"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/fivesevenfivethousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/l"
                android:layout_below="@+id/k"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/nineeightsixthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>



        </RelativeLayout>

    </ScrollView>

</RelativeLayout>
