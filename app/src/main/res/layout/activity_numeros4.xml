<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Numeros4">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:src="@drawable/previous2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|left"
        android:layout_margin="@dimen/fab_margin"
        />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab2"
        android:src="@drawable/next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginLeft="@dimen/fab_margin"
        android:layout_marginTop="@dimen/fab_margin"
        android:layout_marginBottom="@dimen/fab_margin" />

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/a"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/onethousand"
                    android:id="@+id/onethousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/b"
                android:layout_below="@+id/a"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/twothousandtwo"
                    android:id="@+id/twothousandtwo"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/c"
                android:layout_below="@+id/b"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/threethreethreethree"
                    android:id="@+id/threethousandthreethree"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/d"
                android:layout_below="@+id/c"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/fourfourzerofour"
                    android:id="@+id/fourzerozerofour"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/e"
                android:layout_below="@+id/d"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/fivezerofivefive"
                    android:id="@+id/fivezerofivefive"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/f"
                android:layout_below="@+id/e"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/sixsixsixsix"
                    android:id="@+id/sixsixsixsix"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/g"
                android:layout_below="@+id/f"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/sevensevenzeroseven"
                    android:id="@+id/sevensevenzeroseven"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/h"
                android:layout_below="@+id/g"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/eightzeroeighteight"
                    android:id="@+id/eightzeroeighteight"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/i"
                android:layout_below="@+id/h"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/ninenineninenine"
                    android:id="@+id/ninenineninenine"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/j"
                android:layout_below="@+id/i"
                >

                <ImageButton
                    android:layout_width="124sp"
                    android:layout_weight="1"
                    android:layout_height="80dp"
                    android:src="@drawable/tenthousand"
                    android:id="@+id/tenthousand"
                    android:background="#00ffffff"
                    android:scaleType="fitXY"
                    android:layout_gravity="bottom|right"
                    android:paddingLeft="5dp" />


            </LinearLayout>


        </RelativeLayout>


    </ScrollView>




</FrameLayout>
